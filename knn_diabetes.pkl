    "print(\"\\nโมเดลรันสำเร็จ!\")\n",
    "print(f\"Training accuracy: {model.score(X_train, y_train):.4f}\")\n",
    "print(f\"Test accuracy: {model.score(X_test, y_test):.4f}\")"
   ]
