    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.tree import DecisionTreeClassifier\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "\n",
    "# โหลดข้อมูลของคุณ (เปลี่ยน path ตามที่คุณเก็บไฟล์)\n",
    "# df = pd.read_csv('your_data.csv')\n",
    "\n",
    "# สมมติว่าข้อมูลของคุณอยู่ในตัวแปร df แล้ว\n",
    "# และคุณได้แบ่ง X และ y แล้ว\n",
    "# X = df.drop('target_column', axis=1)\n",
    "# y = df['target_column']\n",
    "\n",
    "# แปลงข้อมูลที่เป็นข้อความเป็นตัวเลข\n",
    "print(\"กำลังแปลงข้อมูลที่เป็นข้อความเป็นตัวเลข...\")\n",
    "for col in X.columns:\n",
    "    if X[col].dtype == 'object':\n",
    "        try:\n",
    "            # พยายามแปลงเป็นตัวเลขก่อน\n",
    "            X[col] = pd.to_numeric(X[col])\n",
    "            print(f\"คอลัมน์ '{col}' แปลงเป็นตัวเลขได้\")\n",
    "        except ValueError:\n",
    "            # ถ้าแปลงเป็นตัวเลขไม่ได้ ให้ใช้ LabelEncoder\n",
    "            le = LabelEncoder()\n",
    "            X[col] = le.fit_transform(X[col].astype(str))\n",
    "            print(f\"คอลัมน์ '{col}' ถูกแปลงด้วย LabelEncoder\")\n",
    "\n",
    "# แยกข้อมูล\n",
    "X_train, X_test, y_train, y_test = train_test_split(\n",
    "    X, y, test_size=0.3, random_state=42, stratify=y\n",
    ")\n",
    "\n",
    "# สร้างและเทรนโมเดล\n",
    "model = DecisionTreeClassifier(random_state=42)\n",
    "model.fit(X_train, y_train)\n",
    "\n",
    "# ทำนายผล\n",
    "y_pred = model.predict(X_test)\n",
